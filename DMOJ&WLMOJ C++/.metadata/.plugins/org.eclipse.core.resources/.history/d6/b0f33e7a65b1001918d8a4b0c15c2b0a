#include <bits/stdc++.h>

using namespace std;
int in[1000001];
int freq[1000001];
int main() {
	int n,k;
	scanf("%d %d",&n,&k);
	for(int i=0; i<n; i++){
		scanf("%d",&in[i]);
	}

    return 0;
}
