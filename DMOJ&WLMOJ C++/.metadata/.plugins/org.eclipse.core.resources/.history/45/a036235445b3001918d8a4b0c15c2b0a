#include <bits/stdc++.h>
using namespace std;

int main() {
	cout << "!!!Hello World!!!" << endl; // prints !!!Hello World!!!
	printf("!!!Hello World 123");
	int a;
	scanf("%d",&a);
	printf("%d",a);
	return 0;
}
